{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "java",
			"name": "IntegrationTestApplication",
			"request": "launch",
			"mainClass": "com.young1lin.mybatis.boost.integration.test.IntegrationTestApplication",
			"projectName": "mybatis.boost.integration-test"
		},
		{
			"name": "Run Extension",
			"type": "extensionHost",
			"request": "launch",
			"args": [
				"--extensionDevelopmentPath=${workspaceFolder}"
			],
			"outFiles": [
				"${workspaceFolder}/dist/**/*.js"
			],
			"preLaunchTask": "pnpm: compile"
		}
	]
}
