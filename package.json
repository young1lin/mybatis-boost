{
  "name": "mybatis-boost",
  "displayName": "%displayName%",
  "description": "%description%",
  "version": "0.2.2",
  "publisher": "young1lin",
  "license": "MIT",
  "packageManager": "pnpm@10.19.0",
  "icon": "images/icons/MyBatis.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/young1lin/mybatis-boost.git"
  },
  "engines": {
    "vscode": "^1.99.3"
  },
  "categories": [
    "Programming Languages",
    "Other"
  ],
  "keywords": [
    "mybatis",
    "mapper",
    "xml",
    "java",
    "navigation",
    "sql"
  ],
  "activationEvents": [
    "workspaceContains:**/pom.xml",
    "workspaceContains:**/build.gradle",
    "workspaceContains:**/*.java"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "mybatis-boost-generator",
          "title": "MyBatis Generator",
          "icon": "images/icons/MyBatis.svg"
        }
      ]
    },
    "views": {
      "mybatis-boost-generator": [
        {
          "id": "mybatis-boost.generatorView",
          "name": "Code Generator",
          "type": "webview"
        }
      ]
    },
    "commands": [
      {
        "command": "mybatis-boost.clearCache",
        "title": "%command.clearCache%",
        "category": "MyBatis Boost"
      },
      {
        "command": "mybatis-boost.refreshMappings",
        "title": "%command.refreshMappings%",
        "category": "MyBatis Boost"
      },
      {
        "command": "mybatis-boost.jumpToXml",
        "title": "%command.jumpToXml%"
      }
    ],
    "configuration": {
      "title": "%config.title%",
      "properties": {
        "mybatis-boost.cacheSize": {
          "type": "number",
          "default": 5000,
          "description": "%config.cacheSize.description%"
        },
        "mybatis-boost.customXmlDirectories": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [],
          "description": "%config.customXmlDirectories.description%"
        },
        "mybatis-boost.javaParseLines": {
          "type": "number",
          "default": 100,
          "minimum": 20,
          "maximum": 200,
          "description": "%config.javaParseLines.description%"
        },
        "mybatis-boost.showBindingIcons": {
          "type": "boolean",
          "default": true,
          "description": "%config.showBindingIcons.description%"
        },
        "mybatis-boost.useDefinitionProvider": {
          "type": "boolean",
          "default": false,
          "description": "%config.useDefinitionProvider.description%"
        },
        "mybatis-boost.generator.datetime": {
          "type": "string",
          "enum": [
            "Date",
            "LocalDateTime",
            "Instant"
          ],
          "default": "LocalDateTime",
          "description": "%config.generator.datetime.description%"
        },
        "mybatis-boost.generator.basePackage": {
          "type": "string",
          "default": "com.example.mybatis",
          "description": "%config.generator.basePackage.description%"
        },
        "mybatis-boost.generator.author": {
          "type": "string",
          "default": "MyBatis Boost",
          "description": "%config.generator.author.description%"
        },
        "mybatis-boost.generator.entitySuffix": {
          "type": "string",
          "default": "PO",
          "description": "%config.generator.entitySuffix.description%"
        },
        "mybatis-boost.generator.mapperSuffix": {
          "type": "string",
          "default": "Mapper",
          "description": "%config.generator.mapperSuffix.description%"
        },
        "mybatis-boost.generator.serviceSuffix": {
          "type": "string",
          "default": "Service",
          "description": "%config.generator.serviceSuffix.description%"
        },
        "mybatis-boost.generator.useLombok": {
          "type": "boolean",
          "default": true,
          "description": "%config.generator.useLombok.description%"
        },
        "mybatis-boost.generator.useSwagger": {
          "type": "boolean",
          "default": false,
          "description": "%config.generator.useSwagger.description%"
        },
        "mybatis-boost.generator.useSwaggerV3": {
          "type": "boolean",
          "default": false,
          "description": "%config.generator.useSwaggerV3.description%"
        },
        "mybatis-boost.generator.useMyBatisPlus": {
          "type": "boolean",
          "default": false,
          "description": "%config.generator.useMyBatisPlus.description%"
        },
        "mybatis-boost.mcp.enable": {
          "type": "boolean",
          "default": true,
          "description": "%config.mcp.enable.description%"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "pnpm run package",
    "compile": "pnpm run check-types && pnpm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "pnpm run check-types && pnpm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "pnpm run test:unit && pnpm run test:integration",
    "test:unit": "pnpm exec mocha src/test/unit/**/*.test.ts",
    "test:integration": "vscode-test",
    "release": "node -e \"const v = require('./package.json').version; require('child_process').execSync(`git tag -a v${v} -m \\\"Release ${v}\\\" && git push && git push --tags`, {stdio: 'inherit'})\""
  },
  "devDependencies": {
    "@types/ejs": "^3.1.5",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/sinon": "^17.0.3",
    "@types/vscode": "^1.99.3",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "@vscode/vsce": "^3.6.2",
    "esbuild": "^0.25.10",
    "eslint": "^9.36.0",
    "mocha": "^10.8.2",
    "npm-run-all": "^4.1.5",
    "sinon": "^19.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "ejs": "^3.1.10",
    "sql-ddl-to-json-schema": "6.0.0"
  }
}
